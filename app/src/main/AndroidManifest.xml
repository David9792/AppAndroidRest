<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.developer.david.apprestaurant">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CrearMenu"></activity>
        <activity android:name=".CrearRestaurant" />
        <activity android:name=".RegistroUsuario" />
        <activity
            android:name=".NavigationMenu"
            android:label="@string/title_activity_navigation_menu" />
        <activity
            android:name=".Navegation_Menu"
            android:label="@string/title_activity_navegation__menu" />
        <activity
            android:name=".NavigationRestaurant"
            android:label="@string/title_activity_navigation_restaurant" />
        <activity android:name=".ListRestaurant" />
        <activity android:name=".ListMenu" />
        <activity android:name=".CreateRestaurant" />
        <activity android:name=".CreateMenu" />
        <activity android:name=".LoginAdmin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>